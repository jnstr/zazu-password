{
  "name": "zazu-password",
  "version": "1.0.0",
  "description": "Zazu plugin for generating passwords with ease.",
  "blocks": {
    "input": [
      {
        "id": "password",
        "type": "RootScript",
        "script": "src/password.js",
        "connections": ["Copy"]
      }
    ],
    "output": [
      {
        "id": "Copy",
        "type": "CopyToClipboard",
        "text": "{value}"
      }
    ]
  }
}
